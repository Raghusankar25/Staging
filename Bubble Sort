#include <stdio.h>
int main()
{
    int n;
  int arr[n];
  int i,j,temp;
  printf("enter value of n:");
  scanf("%d",&n);
  printf("enter elements:");
  for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
  for(int i=0;i<n-1;i++)
      {
          for(j=0;j<n-1-i;j++) 
          {
          if(arr[j]>arr[j+1])
          {
              temp=arr[j];
              arr[j]=arr[j+1];
              arr[j+1]=temp;
          }
         }
      }
  printf("final sorted elements are:");
  for(i=0;i<n;i++)
  {
      printf("%d\t",arr[i]);
  }
  return 0;
}


OUTPUT:

enter value of n:5
enter elements:2 8 10 4 9
final sorted elements are:2	4	8	9	10	
Footer
