#include<stdio.h>
int main()
{
    int arr[5]={2,5,3,6,1};
    for(int i=0;i<5;i++)
    {
        int current_ele=arr[i];
        int j=i-1;
        while(j>=0 && arr[j]>current_ele)
        {
            arr[j+1]=arr[j];
            j=j-1;
        }
        arr[j+1]=current_ele;
    }
    for(int i=0;i<5;i++)
        printf("%d\t",arr[i]);
}


OUTPUT:

1       2       3       5       6
