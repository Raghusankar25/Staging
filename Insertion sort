#include<stdio.h>
int main()
{
    int arr[5]={5,2,4,1,3};
    for(int i=0;i<5-1;i++)
    {
        for(int j=i+1;j<5;j++)
        {
            if(arr[i]>arr[j])
            {
                int temp;
                temp=arr[j];
                arr[j]=arr[i];
                arr[i]=temp;
            }
        }
    }
    for(int i=0;i<5;i++)
        printf("%d\t",arr[i]);
}                                 


OUTPUT:

1 2 3 4 5
